{
  "service": {
    "name": "kubernetes-cluster2",
    "sleep": 1000,
    "service_account": "kubernetes-cluster2",
    "service_account_secret": "kubernetes-cluster2/sa",
    "virtual_network_name": "dcos",
    "log_level": "INFO"
  },
  "kubernetes": {
    "authorization_mode": "AlwaysAllow",
    "high_availability": false,
    "use_private_docker_registry": false,
    "service_cidr": "10.100.0.0/16",
    "network_provider": "dcos",
    "calico": {
      "calico_ipv4pool_cidr": "192.168.0.0/16",
      "cni_mtu": 1400,
      "ipv4pool_ipip": "Always",
      "felix_ipinipmtu": 1420,
      "felix_ipinipenabled": true
    },
    "node_count": 5,
    "node_placement": "",
    "public_node_count": 0,
    "public_node_placement": "",
    "reserved_resources": {
      "kube_cpus": 2,
      "kube_mem": 2048,
      "kube_disk": 10240,
      "system_cpus": 1,
      "system_mem": 1024
    },
    "public_reserved_resources": {},
    "control_plane_mem": 4096,
    "control_plane_disk": 10240,
    "control_plane_placement": "[[\"hostname\", \"UNIQUE\"]]",
    "control_plane_cpus": 1
  },
  "etcd": {
    "cpus": 1,
    "mem": 1024,
    "data_disk": 3072,
    "wal_disk": 512,
    "disk_type": "ROOT"
  }
}
